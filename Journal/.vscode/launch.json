{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Electron",
            "type": "chrome",
            "request": "launch",
            "program": "${workspaceRoot}/main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
             "runtimeArgs": [
        "${workspaceRoot}",
        "--enable-logging",
        "--remote-debugging-port=9222"
     ],
            "env": {},
            "console": "integratedTerminal",
            "sourceMaps": false,
            "outDir": null
        }

    ]
}