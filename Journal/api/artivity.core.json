                },
                "IsPluginInstalled": {
                    "type": "boolean"
                },
                "IsPluginEnabled": {
                    "type": "boolean"
                },
                "DetectedVersions": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "A list containing installed versions."
                },
                "AssociationUris": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "A list containing the association uri's."
                }
            }
        },
        "Manifest": {
            "type": "object",
            "properties": {
                "AgentUri": {
                    "type": "string",
                    "format": "URI",
                    "description": "Example http://adobe.com/products/illustrator"
                },
                "DefaultColor": {
                    "type": "string",
                    "format": "RGBA",
                    "description": "Example \"#F27200\""
                },
                "DisplayName": {
                    "type": "string",
                    "description": "Example \"Adobe Illustrator CC 2015\""
                },
                "PluginArch": {
                    "type": "integer",
                    "description": "Architecture of the plugin. Can be 32 or 64"
                },
                "PluginVersion": {
                    "type": "string",
                    "description": "Version of the plugin Example \"0.8.1\""
                }
            }
        },
